	var getStates = function() {
		$.ajax({
			type: "GET",
			url: "/estado/getStates",
			dataType: "json",
			success: function(result) {
				data = result.states
				$("#automplete-2").autocomplete({
				   source: data,
				   messages: false,
				});
			}
		})
	}
	getStates()
</script>
